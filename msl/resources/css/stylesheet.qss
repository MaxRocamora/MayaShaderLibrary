/* **************************************************** */
/* QPROGRESS BAR */
/* **************************************************** */

QProgressBar {
background-color: rgb(30,30,30);
}

QProgressBar:horizontal {
border-radius: 1px;
background-color: rgb(30,30,30);
padding: 1px;
}
QProgressBar::chunk:horizontal {
background: qlineargradient(x1: 0, y1: 0.5, x2: 1, y2: 0.5, stop: 0 #ffc107, stop: 1 #ffb300);
}